<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.internest.com.internest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Blog"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".YourQRCode"
            android:label="@string/title_activity_your_qrcode"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScanCode"
            android:label="@string/title_activity_scan_code"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CaptureActivityPortrait"
            android:label="@string/title_activity_capture_activity_portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".URLDisplay"
            android:label="@string/title_activity_urldisplay"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TextDisplay"
            android:label="@string/title_activity_text_display"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".History"
            android:label="@string/title_activity_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContactUs"
            android:label="@string/title_activity_contact_us"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PointsPromo"
            android:label="@string/title_activity_points_promo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SignUp"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
